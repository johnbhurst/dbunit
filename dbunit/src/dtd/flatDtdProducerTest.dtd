    DUPLICATE_TABLE*,
    SECOND_TABLE*,
    TEST_TABLE*,
    NOT_NULL_TABLE*,
    EMPTY_TABLE*)>

<!ELEMENT TEST_TABLE EMPTY>
<!ATTLIST TEST_TABLE
