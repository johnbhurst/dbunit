    COLUMN0 CDATA #IMPLIED
    COLUMN1 CDATA #IMPLIED
    COLUMN2 CDATA #IMPLIED
    COLUMN3 CDATA #IMPLIED
>

<!ELEMENT NOT_NULL_TABLE EMPTY>
<!ATTLIST NOT_NULL_TABLE
